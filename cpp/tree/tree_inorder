// 二叉树的前序遍历

#include "vector"
#include "stack"

using namespace std;

struct TreeNode {
    int val;
    TreeNode *left;
    TreeNode *right;

    TreeNode() {}
    TreeNode(int val) : val(val) {}
    TreeNode(int val, TreeNode *left, TreeNode *right) : val(val), left(left), right(right) {}
};

class Solution {
public:

    // 递归
    vector<int> inorderTraversal(TreeNode* root) {
        if (root == nullptr) {
            return
        }
        inorder()
    }

private:
    void inorderHelper() {
        
    }
};
